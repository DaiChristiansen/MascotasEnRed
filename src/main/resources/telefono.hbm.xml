<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernateconfiguration-3.0.dtd">
<hibernate-mapping>
    <class name="com.unb001.mascotasenred.modelo.Telefono" table="telefono">
        <id name="idTelefono" column="idTelefono"/>
        <property name="area" column="area"/>
        <property name="telefono" column="telefono"/>
        <property name="tipo" column="tipo"/>
    </class>
</hibernate-mapping>

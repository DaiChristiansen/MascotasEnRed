<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernateconfiguration-3.0.dtd">
<hibernate-mapping>
    <class name="com.unb001.mascotasenred.modelo.Denuncia" table="denuncia">
        <id name="nroDenuncia" column="nroDenuncia"/>
        <property name="tipoDenuncia"/>
        <property name="fecha"/>
        <one-to-one name="comprobante" class="com.unb001.mascotasenred.modelo.Comprobante" lazy="false" fetch="join" cascade="all" constrained="true"/>
        <one-to-one name="mascota" class="com.unb001.mascotasenred.modelo.Mascota" lazy="false" fetch="join" cascade="all" constrained="true"/>          
    </class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernateconfiguration-3.0.dtd">
<hibernate-mapping>
    <class name="com.unb001.mascotasenred.modelo.Denunciante" table="denunciante">
        <id name="idDenunciante" column="idDenunciante"/>
        <property name="dni"/>
        <property name="nombre"/>
        <property name="apellido"/>
        <property name="direccion"/>
        <property name="email"/>
        <property name="telefono"/>
        <one-to-one name="mascota" class="com.unb001.mascotasenred.modelo.Mascota"/> 
        <one-to-one name="denuncia" class="com.unb001.mascotasenred.modelo.Denuncia"/> 
       
    </class>
</hibernate-mapping>
